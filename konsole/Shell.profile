[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=YYC
Font=DejaVu Sans Mono,12,-1,5,50,0,0,0,0,0

[Cursor Options]
UseCustomCursorColor=true

[General]
Command=/bin/zsh --login
Directory=~
Environment=TERM=xterm-256color
Icon=utilities-terminal
LocalTabTitleFormat=%w
MouseWheelZoomEnabled=false
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%h : %w
SaveGeometryOnExit=true
ShowMenuBar=false
ShowTerminalSizeHint=false
StartInCurrentSessionDir=true
TabBarMode=1

[Interaction Options]
CtrlRequiredForDrag=true
MouseWheelZoomEnabled=false
TrimTrailingSpacesInSelectedText=true
TripleClickMode=0

[Keyboard]
KeyBindings=linux

[Scrolling]
HistorySize=65536

[Terminal Features]
BidiRenderingEnabled=false
BlinkingCursorEnabled=true
FlowControlEnabled=false
